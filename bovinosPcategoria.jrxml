	<parameter name="id_empresa" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[SELECT b.descricao AS categoria,
       a.sexo,
       COUNT(a.id_animal) AS qtde,
       AVG(a.peso_atual) AS peso_medio,
       SUM(a.peso_atual)/450 AS UA,
       SUM(a.peso_atual)/30 AS Arroba
FROM xgp_animal a
JOIN xgp_categoria b ON a.id_categoria = b.id_categoria
join empresa on empresa.id_empresa = a.id_empresa
WHERE a.id_especie = 1
  AND a.status = 'E'
  AND empresa.id_empresa = $P{id_empresa}
GROUP BY b.descricao, a.sexo
ORDER BY a.sexo, b.descricao



	 
]]></query>
