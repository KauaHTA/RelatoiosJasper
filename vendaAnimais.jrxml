	<query language="SQL"><![CDATA[WITH pesagem_ordenada AS (
    SELECT 
        id_animal,
        data_pesagem,
        peso,
        ROW_NUMBER() OVER (PARTITION BY id_animal ORDER BY data_pesagem DESC) AS rn
    FROM vw_xgp_pesagem
)
SELECT 
    a.id_mov,
    a.valor_acrescimo,a.valor_desconto,
    COALESCE(e.nome_fantasia, e.nome) AS nome_cliente,
    a.data_movimento AS data,
    CAST(b.peso AS NUMERIC) AS peso_atual,
    c.data_pesagem AS data_peso_ant,
    CAST(c.peso AS NUMERIC) AS peso_anterior,
    (b.peso - c.peso) AS ganho_peso,
    ROW_NUMBER() OVER (ORDER BY b.cod_animal) AS num,

    CAST(
        ROUND(
            (b.peso - c.peso) / NULLIF((a.data_movimento - c.data_pesagem), 0),
            2
        ) AS NUMERIC(12,2)
    ) AS gmd,

    CAST(b.valor_kg AS NUMERIC) AS valor_kg,
    b.valor_arroba,
    b.valor_total,
    b.valor_liquido,
    b.cod_animal AS idv,
    b.cod_botom AS ide,
    b.data_nascimento AS nascimento,
    f.descricao AS raca,
    COALESCE(NULLIF(h.nome, ''), h.cod_animal, 'Sem registro') AS pai,
    m.cod_animal AS idv_mae,
    

    COALESCE(b.sexo, 'Não informado') AS sexo

FROM 
    xgp_mov a
    INNER JOIN xgp_mov_animal b ON b.id_mov = a.id_mov
    LEFT JOIN pesagem_ordenada c ON c.id_animal = b.id_animal AND c.rn = 2 
    INNER JOIN xgp_operacao d ON d.id_operacao = a.id_operacao
    INNER JOIN xgp_pessoa e ON e.id_pessoa = a.id_pessoa
    LEFT JOIN xgp_raca f ON f.id_raca = b.id_raca
    LEFT JOIN xgp_animal g ON g.id_animal = b.id_animal
    LEFT JOIN xgp_animal h ON h.id_animal = g.id_animal_pai 
    LEFT JOIN xgp_animal m ON m.id_animal = g.id_animal_mae
WHERE 
    d.natureza = 'S'
    AND a.id_mov = $P{id_mov}
  ]]></query>
